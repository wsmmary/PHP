class Solution {

    /**
     * @param Integer[] $nums
     * @return Integer
     */
    function maxSubArray($nums) {
        $res = $nums[0];
        $sum = 0;
        foreach ($nums as $num) {
            if ($sum > 0)
                $sum += $num;
            else
                $sum = $num;
            $res = max($res, $sum);
        }
        return $res;

    }
}
